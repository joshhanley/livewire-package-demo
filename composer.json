{
    "name": "joshhanley/livewire-package-demo",
    "description": "A demo for how to use Livewire Dusk Testbench to test a Livewire component package.",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Hanley",
            "email": "josh@hitsystems.com.au"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "joshhanley/livewire-dusk-testbench": "^3.0.0-beta"
    },
    "autoload": {
        "psr-4": {
            "LivewirePackageDemo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LivewirePackageDemo\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LivewirePackageDemo\\LivewirePackageDemoServiceProvider"
            ]
        }
    }
}
